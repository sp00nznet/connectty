# Connectty Environment Configuration

# Database
DB_PASSWORD=connectty_secret

# JWT Configuration
JWT_SECRET=change-this-in-production-to-a-random-string
JWT_EXPIRY=24h

# Master key for credential encryption (auto-generated if not provided)
MASTER_KEY=

# Web client port
WEB_PORT=8080

# CORS origin (use specific domain in production)
CORS_ORIGIN=*

# Active Directory Authentication (optional)
AD_ENABLED=false
AD_URL=ldap://your-ad-server:389
AD_BASE_DN=DC=corp,DC=example,DC=com
AD_DOMAIN=corp.example.com
